    <ApplicationData> string </ApplicationData>
    <AutoPay> boolean </AutoPay>
    <BestOfferDetails> BestOfferDetailsType
      <BestOfferEnabled> boolean </BestOfferEnabled>
    </BestOfferDetails>
    <BuyerRequirementDetails> BuyerRequirementDetailsType
